# Xish AI Backend Environment
XISH_ENVIRONMENT=development
XISH_DEBUG=true

# CORS
XISH_CORS_ORIGINS=["http://localhost:3000","exp://127.0.0.1:8081","http://localhost:8081"]

# Database
XISH_DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/postgres

# Supabase
XISH_SUPABASE_URL=https://YOUR-PROJECT.supabase.co
XISH_SUPABASE_ANON_KEY=YOUR-ANON-KEY
XISH_SUPABASE_SERVICE_ROLE_KEY=YOUR-SERVICE-ROLE
XISH_SUPABASE_JWKS_URL=https://YOUR-PROJECT.supabase.co/auth/v1/keys

# AI Providers
XISH_OPENAI_API_KEY=
XISH_DEFAULT_MODEL=gpt-4o-mini

# Security
XISH_JWT_AUD=
XISH_JWT_ISS=
